{
    "name": "express",
    "version": "0.1.0",
    "description": "express server",
    "main": "index.js",
    "type": "module",
    "author": "KIPRI",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon src/index.ts",
        "db:push": "drizzle-kit push:sqlite --config=drizzle.config.ts",
        "db:migrate": "drizzle-kit generate:sqlite --config=drizzle.config.ts",
        "db:studio": "drizzle-kit studio --port 5556 --verbose",
        "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
        "db:up": "drizzle-kit up:sqlite --config=drizzle.config.ts",
        "db:check": "drizzle-kit check:sqlite --config=drizzle.config.ts"
    },
    "dependencies": {
        "@libsql/client": "^0.3.1",
        "@t3-oss/env-core": "^0.6.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "drizzle-orm": "^0.28.5",
        "drizzle-zod": "^0.5.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.7",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.8",
        "drizzle-kit": "^0.19.13",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
